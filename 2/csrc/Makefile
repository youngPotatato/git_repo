CC=$(TMPSPECENV) $(VCS_CC) $(TMPSPECARG)

# C flags for compilation
CFLAGS=-w  -pipe -m32 -O -I/home/yy/EDA/VCS2016/include   

CFLAGS_CG=-w   -pipe -m32 -I/home/yy/EDA/VCS2016/include -O  

