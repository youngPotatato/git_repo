# Choose name of executable 
PRODUCTBASE=$(VSRC)/simv1

PRODUCT=$(PRODUCTBASE)

# Product timestamp file. If product is newer than this one,
# we will also re-link the product.
PRODUCT_TIMESTAMP=product_timestamp

# Path to runtime library
DEPLIBS=
VCSUCLI=/home/yy/EDA/VCS2016/linux/lib/libvcsucli.so
RUNTIME=/home/yy/EDA/VCS2016/linux/lib/libvcsnew.so /home/yy/EDA/VCS2016/linux/lib/libsimprofile.so /home/yy/EDA/VCS2016/linux/lib/libuclinative.so $(DEPLIBS) 

VCS_SAVE_RESTORE_OBJ=/home/yy/EDA/VCS2016/linux/lib/vcs_save_restore_new.o

# Select your favorite compiler

# Linux:
VCS_CC=gcc

# Internal CC for gen_c flow:
CC_CG=gcc
# User overrode default CC: 
VCS_CC=gcc
# Loader
LD=g++

# Strip Flags for target product
STRIPFLAGS= 
# Loader Flags
LDFLAGS= -m32 -m32 -LDFLAGS-wl,--rpth,/home/yy/EDA/disk2/verdi2016_06/share/PLI/VCS/LINUX -rdynamic 
# Picarchive Flags
PICLDFLAGS=-Wl,-rpath-link=./ -Wl,-rpath='$$ORIGIN'/simv1.daidir/ -Wl,-rpath=./simv1.daidir/ -Wl,-rpath='$$ORIGIN'/simv1.daidir//scsim.db.dir 

# C run time startup
CRT0=
# C run time startup
CRTN=
# Machine specific libraries
SYSLIBS=/home/yy/EDA/disk2/verdi2016_06/share/PLI/VCS/LINUX/pli.a /home/yy/EDA/VCS2016/linux/lib/ctype-stubs_32.a -ldl  -lc -lm -lpthread -ldl

# Default defines
