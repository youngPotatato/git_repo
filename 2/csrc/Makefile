LIBS=/home/yy/EDA/VCS2016/linux/lib/libzerosoft_rt_stubs.so /home/yy/EDA/VCS2016/linux/lib/libvirsim.so /home/yy/EDA/VCS2016/linux/lib/liberrorinf.so /home/yy/EDA/VCS2016/linux/lib/libsnpsmalloc.so 
